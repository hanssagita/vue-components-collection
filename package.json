{
  "name": "vue-components-collection",
  "version": "1.2.0",
  "description": "vue-components-collection is a toolkit component library",
  "author": "Hans Sagita < hans.sagita@gmail.com >",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanssagita/vue-components-collection"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "npm run serve"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "node-sass": "^7.0.0",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bit": {
    "env": {
      "compiler": "bit.envs/bundlers/vue@2.6.21"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
